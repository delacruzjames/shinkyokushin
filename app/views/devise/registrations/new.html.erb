<h2>Registration From</h2>

<%= form_for(resource, :as => resource_name, :url => registration_path(resource_name)) do |f| %>
  <%= devise_error_messages! %>
  <p>
  	<%= f.label :first_name %>
  	<%= f.text_field :first_name %>
  </p>
  <p>
  	<%= f.label :middle_name %>
  	<%= f.text_field :middle_name %>
  </p>
  <p>
  	<%= f.label :last_name %>
  	<%= f.text_field :last_name %>
  </p>
  <p>
  	<%= f.label :age %>
  	<%= f.text_field :age %>
  </p>
  <p>
  	<%= f.label :gender %>
  	<%= f.text_field :gender %>
  </p>
  <p>
  	<%= f.label :contact_number %>
  	<%= f.text_field :contact_number %>
  </p>
  <p>
  	<%= f.label :add_line_1 %>
  	<%= f.text_field :add_line_1 %>
  </p>
  <p>
  	<%= f.label :add_line_2 %>
  	<%= f.text_field :add_line_2 %>
  </p>
  <p>
  	<%= f.label :add_line_3 %>
  	<%= f.text_field :add_line_3 %>
  </p>
  <p>
  	<%= f.label :address_full %>
  	<%= f.text_field :address_full %>
  </p>
  <p>
  	<%= f.label :contact_person_fullname %>
  	<%= f.text_field :contact_person_fullname %>
  </p>
  <p>
  	<%= f.label :contact_person_number %>
  	<%= f.text_field :contact_person_number %>
  </p>
  <p>
  	<%= f.label :contact_person_email %>
  	<%= f.text_field :contact_person_email %>
  </p>
  <p>
  	<%= f.label :contact_person_address %>
  	<%= f.text_field :contact_person_address %>
  </p>
  <p>
  	<%= f.label :email %>
  	<%= f.email_field :email, :autofocus => true %>
  </p>

  <p>
  	<%= f.label :password %>
  	<%= f.password_field :password %>
  </p>

  <p>
  	<%= f.label :password_confirmation %>
  	<%= f.password_field :password_confirmation %>
  </p>

  <div><%= f.submit "Sign up" %></div>

<% end %>

<%= render "devise/shared/links" %>
